{
	"pluginSearchDirs": ["."],
	"semi": true,
	"printWidth": 100,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"singleQuote": true,
	"quoteProps": "consistent",
	"importOrder": [
		"^react$",
		"^next$",
		"^react/(.*)$",
		"^next/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^~/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
